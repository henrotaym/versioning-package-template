{
    "name": "henrotaym/versioning-package-template",
    "description": "Basic versioned php package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Henrotaym\\VersioningPackageTemplate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Henrotaym\\VersioningPackageTemplate\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Henrotaym\\VersioningPackageTemplate\\Providers\\VersioningPackageTemplateServiceProvider"
            ]
        }
    }
}
